{
  "name": "magento-connector",
  "version": "1.7.1",
  "description": "NPM tools to aid in developing this connector application",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@code-ssh.bloomreach.com:engineering/sm/magento-discovery-connector.git"
  },
  "scripts": {
    "deploy-qa": "ts-node --transpile-only bin/main deploy-qa \"/home/magento/server\" Bloomreach/Connector",
    "deploy-dev": "ts-node --transpile-only bin/main deploy-dev Bloomreach/Connector",
    "dev": "ts-node --transpile-only bin/main dev Bloomreach/Connector",
    "magento-install": "ts-node --transpile-only bin/main magento-install",
    "magento-post-install": "ts-node --transpile-only bin/main magento-post-install",
    "magento-uninstall": "ts-node --transpile-only bin/main magento-uninstall",
    "magento-sync-plugin": "ts-node --transpile-only bin/main magento-sync-plugin",
    "pr": "ts-node --transpile-only bin/main url \"https://code.bloomreach.com/engineering/sm/magento-discovery-connector\"",
    "pr-release": "ts-node --transpile-only bin/main pr-release \"https://code.bloomreach.com/engineering/sm/magento-discovery-connector\" gitlab",
    "release": "ts-node --transpile-only bin/main release",
    "ts": "ts-node --transpile-only bin/main ts",
    "view-release": "node bin/main release-notes",
    "dns": "ts-node --transpile-only bin/main dns",
    "command-help": "ts-node --transpile-only bin/main"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "change-case": "4.1.2",
    "colors": "1.4.0",
    "commander": "10.0.0",
    "enquirer": "2.3.6",
    "fs-extra": "11.1.0",
    "hostile": "1.3.3",
    "lodash": "4.17.21",
    "node-ssh": "13.0.0",
    "nodemon": "2.0.20",
    "open": "8.4.0",
    "request": "2.88.2",
    "rimraf": "4.1.1",
    "rsync": "0.6.1",
    "semver": "7.3.8",
    "shelljs": "0.8.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "valid-url": "1.0.9"
  }
}
